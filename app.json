{
  "expo": {
    "name": "playnite",
    "slug": "playnite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/playnite-icon.svg",
    "scheme": "playnite",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#d41173",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.roshan.playnite"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/playnite-icon.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/playnite-icon.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d41173",
          "dark": {
            "backgroundColor": "#d41173"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
